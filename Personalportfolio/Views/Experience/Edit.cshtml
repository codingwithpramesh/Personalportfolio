@model Experience

@{
    ViewData["Title"] = "Edit Experience";
}


<form asp-action="Edit" method="post" enctype="multipart/form-data">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="row">
        <div class="col-md-8 offset-2">
            <div class="form-group">
                <label asp-for="Id">Id</label>
                <input asp-for="Id" class="form-control" readonly />
                <span asp-validation-for="Id"></span>
            </div>
            <div class="form-group">
                <label asp-for="Company">Company</label>
                <input asp-for="Company" class="form-control" />
                <span asp-validation-for="Company"></span>
            </div>

            <div class="form-group">
                <label asp-for="Position"> Position</label>
                <input type="datetime" asp-for="Position" class="form-control" />
                <span asp-validation-for="Position"></span>
            </div>

            <div class="form-group">
                <label asp-for="JoinDate"> JoinDate</label>
                <input asp-for="JoinDate" class="form-control" />
                <span asp-validation-for="JoinDate"></span>
            </div>

            <div class="form-group">
                <label asp-for="EndDate"> EndDate</label>
                <input type="datetime" asp-for="EndDate" class="form-control" />
                <span asp-validation-for="EndDate"></span>
            </div>



            <div class="form-group">
                <label asp-for="Description">Description</label>
                <input asp-for="Description" class="form-control" />
                <span asp-validation-for="Description"></span>
            </div>

            <div class="form-group">
                <label asp-for="UId"> UId</label>
                <input asp-for="UId" class="form-control" />
                <span asp-validation-for="UId"></span>
            </div>
            <div class="form-group">
                <button type="submit" asp-action="Edit" class="btn btn-primary">Edit</button>
            </div>
        </div>
    </div>

</form>
