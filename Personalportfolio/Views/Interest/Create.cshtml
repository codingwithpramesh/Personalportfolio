@model Interest

@{
    ViewData["Title"] = "Create Interest";
}


<form asp-action="Create" method="post" enctype="multipart/form-data">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="row">
        <div class="col-md-8 offset-2">
           
         

            <div class="form-group">
                <label asp-for="Desription">Description</label>
                <input asp-for="Desription" class="form-control" />
                <span asp-validation-for="Desription"></span>
            </div>

            <div class="form-group" style="margin-top:20px;">
                <button type="submit" asp-action="Create" class="btn btn-primary">Create</button>
            </div>
        </div>
    </div>

</form>
