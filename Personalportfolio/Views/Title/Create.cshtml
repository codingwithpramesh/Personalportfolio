@model Title

@{
    ViewData["Title"] = "Create Title";
}


<form asp-action="Create" method="post" enctype="multipart/form-data">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="row">
        <div class="col-md-8 offset-2">
           
            <div class="form-group">
                <label asp-for="Experience">Experience</label>
                <input asp-for="Experience" class="form-control" />
                <span asp-validation-for="Experience"></span>
            </div>


            <div class="form-group">
                <label asp-for="Education"> Education</label>
                <input asp-for="Education" class="form-control" />
                <span asp-validation-for="Education"></span>
            </div>

            <div class="form-group">
                <label asp-for="Skills"> Skills</label>
                <input asp-for="Skills" class="form-control" />
                <span asp-validation-for="Skills"></span>
            </div>

            <div class="form-group">
                <label asp-for="ProgrammingLanguage"> Prrogramming language</label>
                <input asp-for="ProgrammingLanguage" class="form-control" />
                <span asp-validation-for="ProgrammingLanguage"></span>
            </div>



            <div class="form-group">
                <label asp-for="workflows">WorkFlows</label>
                <input asp-for="workflows" class="form-control" />
                <span asp-validation-for="workflows"></span>
            </div>



            <div class="form-group">
                <label asp-for="Interest">Interest</label>
                <input asp-for="Interest" class="form-control" />
                <span asp-validation-for="Interest"></span>
            </div>



            <div class="form-group">
                <label asp-for="Awards">Awards</label>
                <input asp-for="Awards" class="form-control" />
                <span asp-validation-for="Awards"></span>
            </div>


            <div class="form-group">
                <label asp-for="UId"> UId</label>
                <input asp-for="UId" class="form-control" />
                <span asp-validation-for="UId"></span>
            </div>
            <div class="form-group">
                <button type="submit" asp-action="Create" class="btn btn-primary">Create</button>
            </div>
        </div>
    </div>

</form>
