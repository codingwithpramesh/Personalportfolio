@model Awards

@{
    ViewData["Title"] = "Delete award";
}


<form asp-action="Delete" method="post" enctype="multipart/form-data">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="row">
        <div class="col-md-8 offset-2">
            <div class="form-group">
                <label asp-for="Id">Id</label>
                <input type="hidden" asp-for="Id" class="form-control"  disabled/>
                <span asp-validation-for="Id"></span>
            </div>
            <div class="form-group">
                <label asp-for="Description">Description</label>
                <input asp-for="Description" class="form-control" />
                <span asp-validation-for="Description"></span>
            </div>

        

            <div class="form-group" style="margin-top:20px;">
                <button type="submit" asp-action="Delete" class="btn btn-primary">Delete</button>
            </div>
        </div>
    </div>

</form>
